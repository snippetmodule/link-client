{
  "name": "react-native-ts",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "prestart": "tsc --watch &",
    "stop": "sudo kill -9 $(lsof -a -c node -t -i:8081)",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "android": "adb reverse tcp:8081 tcp:8081 && node node_modules/react-native/local-cli/cli.js run-android",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "prebuild": "rm -rf build",
    "build": "tsc"
  },
  "dependencies": {
    "crc32": "^0.2.2",
    "moment": "^2.16.0",
    "parse": "^1.9.2",
    "parse-dashboard": "~1.0.8",
    "parse-server": "~2.2.6",
    "react": "15.3.1",
    "react-native": "^0.37.0",
    "react-native-button": "^1.6.0",
    "react-native-code-push": "^1.16.0-beta",
    "react-native-i18n": "^0.1.1",
    "react-native-linear-gradient": "^1.5.13",
    "react-native-push-notification": "^2.2.0",
    "react-native-send-intent": "^1.0.13",
    "react-native-share": "^1.0.17",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-persist": "^4.0.0-beta1",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4"
  },
  "devDependencies": {
    "@types/react": "^0.14.49",
    "@types/react-native": "^0.34.39",
    "@types/react-redux": "^4.4.32",
    "@types/redux": "^3.6.31",
    "@types/parse": "^1.2.32",
    "tslint": "3.15.1",
    "tslint-react": "^1.1.0",
    "typescript": "^2.2.0-dev.20161113"
  }
}
