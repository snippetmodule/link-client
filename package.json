{
  "name": "react-native-ts",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "prestart": "tsc --watch &",
    "stop": "sudo kill -9 $(lsof -a -c node -t -i:8081)",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "android": "adb reverse tcp:8081 tcp:8081 && node node_modules/react-native/local-cli/cli.js run-android",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "prebuild": "rm -rf build",
    "build": "tsc"
  },
  "dependencies": {
    "crc32": "^0.2.2",
    "moment": "^2.17.1",
    "parse": "^1.9.2",
    "parse-dashboard": "^1.0.23",
    "parse-server": "~2.3.2",
    "react": "^15.4.2",
    "react-native": "^0.46.2",
    "react-native-google-analytics-bridge": "^5.0.0",
    "react-native-i18n": "^2.0.2",
    "react-native-linear-gradient": "^2.0.0",
    "react-native-push-notification": "^3.0.0",
    "react-native-send-intent": "^1.0.14",
    "react-native-share": "^1.0.19",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.2.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@types/parse": "^1.2.32",
    "@types/react": "^15.0.6",
    "@types/react-native": "^0.46.2",
    "@types/react-redux": "^4.4.36",
    "@types/redux": "^3.6.31",
    "@types/redux-logger": "^3.0.0",
    "tslint": "^4.4.2",
    "tslint-react": "^3.0.0",
    "typescript": "^2.1.5"
  }
}
